{
  "extends": [
    "stylelint-config-standard-scss",
    "stylelint-config-property-sort-order-smacss"
  ],
  "plugins": [
    "stylelint-scss",
    "stylelint-order",
    "stylelint-no-unused-selectors"
  ],
  "rules": {
    "property-no-vendor-prefix": null,
    "selector-class-pattern": null,
    "no-descending-specificity": null
  }
}
